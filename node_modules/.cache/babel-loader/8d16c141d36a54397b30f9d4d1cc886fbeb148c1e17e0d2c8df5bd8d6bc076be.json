ng = true;\n        state.error = null;\n      })\n      .addCase(getWeatherThunk.fulfilled, (state, action) => {\n        state.isLoading = false;\n        state.weather = action.payload;\n        state.error = null;\n      })\n      .addCase(getWeatherThunk.rejected, (state, action) => {\n        state.isLoading = false;\n        state.error = action.payload;\n      });\n  },\n});\n\nexport const { addToFavorites, removeFromFavorites } = weatherSlice.actions;\nexport const weatherReducer = weatherSlice.reducer;\n"],"mappings":"AAAA,OAASA,WAAW,KAAQ,kBAAkB,CAC9C,OAASC,eAAe,KAAQ,aAAa,CAE7C,KAAM,CAAAC,YAAY,CAAG,CACnBC,OAAO,CAAE,EAAE,CACXC,YAAY,CAAE,CAAC,CAAC,CAChBC,SAAS,CAAE,EACb,CAAC,CAED,KAAM,CAAAC,YAAY,CAAGN,WAAW,CAAC,CAC/BO,IAAI,CAAE,SAAS,CACfL,YAAY,CACZM,QAAQ,CAAE,CACRC,cAAc,CAAEA,CAACC,KAAK,CAAEC,MAAM,GAAK,CACjCD,KAAK,CAACL,SAAS,CAAG,CAAC,GAAGK,KAAK,CAACL,SAAS,CAAEM,MAAM,CAACC,OAAO,CAAC,CACxD,CAAC,CACDC,mBAAmB,CAAEA,CAACH,KAAK,CAAEC,MAAM,GAAK,CACtC,KAAM,CAAEG,KAAM,CAAC,CAAGH,MAAM,CAACC,OAAO,CAChCF,KAAK,CAACL,SAAS,CAACU,MAAM,CAACD,KAAK,CAAE,CAAC,CAAC,CAClC,CACF,CAAC,CACDE,aAAa,CAAGC,OAAO,EAAK,CAC1BA,OAAO,CACJC,OAAO,CAACjB,eAAe,CAACkB,OAAO,CAAGT,KAAK,EAAK,CAC3CA,KAAK,CAACU,SAAS,CAAG,IAAI,CACtBV,KAAK,CAACW,KAAK,CAAG,IAAI,CACpB,CAAC,CAAC,CACDH,OAAO,CAACjB,eAAe,CAACqB,SAAS,CAAE,CAACZ,KAAK,CAAEC,MAAM,GAAK,CACrDD,KAAK,CAACU,SAAS,CAAG,KAAK,CACvBV,KAAK,CAACP,OAAO,CAAGQ,MAAM,CAACC,OAAO,CAC9BF,KAAK,CAACW,KAAK,CAAG,IAAI,CACpB,CAAC,CAAC,CACDH,OAAO,CAACjB,eAAe,CAACsB,QAAQ,CAAE,CAACb,KAAK,CAAEC,MAAM,GAAK,CACpDD,KAAK,CAACU,SAAS,CAAG,KAAK,CACvBV,KAAK,CAACW,KAAK,CAAGV,MAAM,CAACC,OAAO,CAC9B,CAAC,CAAC,CACN,CACF,CAAC,CAAC,CAEF,MAAO,MAAM,CAAEH,cAAc,CAAEI,mBAAoB,CAAC,CAAGP,YAAY,CAACkB,OAAO,CAC3E,MAAO,MAAM,CAAAC,cAAc,CAAGnB,YAAY,CAACoB,OAAO"},"metadata":{},"sourceType":"module","externalDependencies":[]}